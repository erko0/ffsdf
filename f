      get isiOSMobile() {
        const e = typeof window == "object" && navigator.maxTouchPoints > 2;
        return /iPhone|iPad|iPod/i.test(this.userAgent) || /Macintosh/i.test(this.userAgent) && e;
      }
      get isOperaMini() {
        return /Opera Mini/i.test(this.userAgent);
      }
      get isHarmony() {
