    function zk(e, t) {
      const n = e.canvas.width;
      const r = e.canvas.height;
      let a = null;
      let o = null;
      a = typeof a == "number" ? a : 0.5;
      o = typeof o == "number" ? o : 0.5;
      if (a < 0) {
        a = 0;
      }
      if (o < 0) {
        o = 0;
      }
      if (a > 1) {
        a = 1;
      }
      if (o > 1) {
        o = 1;
      }
      const i = t.videoWidth;
      const l = t.videoHeight;
      const s = Math.min(n / i, r / l);
      let c;
      let u;
      let d;
      let f;
      let m = i * s;
      let p = l * s;
      let h = 1;
      if (m < n) {
        h = n / m;
      }
      if (Math.abs(h - 1) < 1e-14 && p < r) {
        h = r / p;
      }
      m *= h;
      p *= h;
      d = i / (m / n);
      f = l / (p / r);
      c = (i - d) * a;
      u = (l - f) * o;
      if (c < 0) {
        c = 0;
      }
      if (u < 0) {
        u = 0;
      }
      if (d > i) {
        d = i;
      }
      if (f > l) {
        f = l;
      }
      e.drawImage(t, c, u, d, f, 0, 0, n, r);
    }
