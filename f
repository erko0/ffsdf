    class qu {
      constructor(e, t = Ku.LOG) {
        this._name = e;
        this._level = t;
      }
      _getLevel() {
        if (typeof this._level == "function") {
          return this._level();
        } else {
          return this._level;
        }
      }
      get name() {
        if (typeof this._name == "string") {
          return this._name;
        } else {
          return this._name();
        }
      }
      isGrouped(e) {
        return e.length > 1 && typeof e[0] == "string";
      }
      grouped(e) {
        console.group(e);
        for (var t = arguments.length, n = new Array(t > 1 ? t - 1 : 0), r = 1; r < t; r++) {
          n[r - 1] = arguments[r];
        }
        console.log(n);
        console.groupEnd();
      }
      log() {
        if ((this._getLevel() & Ku.LOG) === Ku.LOG) {
          for (var e = arguments.length, t = new Array(e), n = 0; n < e; n++) {
            t[n] = arguments[n];
          }
          if (this.isGrouped(t)) {
            const [e, ...n] = t;
            this.grouped(`[1m[32m[${this.name}]: ${e}[0m`, ...n);
          } else {
            console.log(`[1m[32m[${this.name}]: [0m${t}`);
          }
        }
      }
      warn() {
        if ((this._getLevel() & Ku.WARN) === Ku.WARN) {
          for (var e = arguments.length, t = new Array(e), n = 0; n < e; n++) {
            t[n] = arguments[n];
          }
          if (this.isGrouped(t)) {
            const [e, ...n] = t;
            this.grouped(`[1m[33m[${this.name}]: ${e}[0m`, ...n);
          } else {
            console.log(`[1m[33m[${this.name}]: [0m${t}`);
          }
        }
      }
      error() {
        if ((this._getLevel() & Ku.ERROR) === Ku.ERROR) {
          for (var e = arguments.length, t = new Array(e), n = 0; n < e; n++) {
            t[n] = arguments[n];
          }
          if (this.isGrouped(t)) {
            const [e, ...n] = t;
            this.grouped(`[1m[31m[${this.name}]: ${e}[0m`, ...n);
          } else {
            console.log(`[1m[31m[${this.name}]: [0m${t}`);
          }
        }
      }
      set level(e) {
        this._level = e;
      }
    }
